def chiqar_x_y():
    x = input("x sonini kiriting: ")
    y = input("y sonini kiriting: ")
    
    print(x)
    print(y)
    print(x)
    print(y)
chiqar_x_y()
#############################################################################

def solishtir_sonlar():
    x = float(input("Birinchi sonni kiriting: "))
    y = float(input("Ikkinchi sonni kiriting: "))

    if x > y:
        print(f"Kattasi: {x}")
    elif y > x:
        print(f"Kattasi: {y}")
    else:
        print("Sonlar teng")


solishtir_sonlar()


