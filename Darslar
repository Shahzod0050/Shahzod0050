# tugilgan_yil.py

from datetime import datetime

yosh = int(input("Yoshingiz nechida? "))
hozirgi_yil = datetime.now().year
tugilgan_yil = hozirgi_yil - yosh
print(f"Siz taxminan {tugilgan_yil}-yilda tug‘ilgansiz.")

