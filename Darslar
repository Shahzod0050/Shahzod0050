# Davlatlar haqida lug'at
davlatlar = {
    'O‘zbekiston': {
        'poytaxt': 'Toshkent',
        'maydon': 448978,
        'aholi': 36_000_000,
        'pul birligi': "so'm"
    },
    'AQSh': {
        'poytaxt': 'Vashington',
        'maydon': 9834000,
        'aholi': 331_000_000,
        'pul birligi': 'dollar'
    },
    'Germaniya': {
        'poytaxt': 'Berlin',
        'maydon': 357386,
        'aholi': 83_000_000,
        'pul birligi': 'yEVRO'
    },
    'Yaponiya': {
        'poytaxt': 'Tokio',
        'maydon': 377975,
        'aholi': 125_000_000,
        'pul birligi': 'yen'
    }
}

# Har bir davlat haqida ma'lumotni chiqarish
for davlat, info in davlatlar.items():
    print(f"\nDavlat: {davlat}")
    print(f"Poytaxt: {info['poytaxt']}")
    print(f"Maydoni: {info['maydon']} km²")
    print(f"Aholisi: {info['aholi']:,} kishi")
    print(f"Pul birligi: {info['pul birligi']}")
