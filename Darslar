kitoblar = []

print("Yaxshi ko‘rgan kitoblaringizni kiriting.")
print("To‘xtatish uchun 'stop' deb yozing.")

while True:
    kitob = input("Kitob nomi: ")
    if kitob.lower() == 'stop':
        break
    kitoblar.append(kitob)

# Kirilgan kitoblar ro'yxatini chiqaramiz
print("\nSiz yaxshi ko‘rgan kitoblar:")
for nom in kitoblar:
    print(f"- {nom}")

